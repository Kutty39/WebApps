package com.blbz.webapageapp.serviceimp;

import static com.blbz.webapageapp.model.WebPageAppModel.*;

import javax.servlet.http.HttpServletRequest;

import com.blbz.webapageapp.service.WebPageApp;

public class WebPageAppImpl implements WebPageApp {
	@Override
	public boolean emailchecker(String email) {
		String qry=selectQryBuilder().setColumname("*").setTablename("user_info").setWhere("user_email", "=",email).build();
		return dbAction().checkStatement(qry);
	}

	@Override
	public boolean registerUser(HttpServletRequest request) {
		
		int i= dbAction().registerUSer(sql, request);
		return i>0;
	}
}
